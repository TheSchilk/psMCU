# Enable Interrupts
MAIN: LITA 0x1
SVA 0x106

# Loop that cycles values through A
Loop: LITA 0xF0
LITA 0xF1
LITA 0xF2
LITA 0xF3
LITA 0xF4
LITA 0xF5
LITA 0xF6
LITA 0xF7
LITA 0xF8
LITA 0xF9
JMP Loop

# Interrupt that puts A through 4 other values
# Keep A at 0x11
INTERRUPT: LITA 0x11
LITA 0x12
LITA 0x13
LITA 0x14
RTRNI
