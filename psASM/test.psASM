@define ONE axeh
@define ONE1 ONE
@define ONE2 ONE1
@define ONE3 ONE2


# Enable Interrupts
MAIN:
LITA ONE3
SVA 0x102
JMP START


# Loop that cycles values through A
START:
Loop: LITA 0xF0
LITA 1
LITA 0xF2
LITA 0xF3
LITA 0xF4
LITA 0xF5
LITA 0xF6
LITA 0xF7
LITA 0xF8
LITA 0xF9
JMP Loop

# Interrupt that puts A through 4 other values
# Keep A at 0x11
INTERRUPT:
LITA 0x11
LITA 0x12
LITA 0x13
LITA 0x14
RTRNI

Danger:
